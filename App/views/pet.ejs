<!DOCTYPE html>
<html>
  <head>
		<!-- Page Title -->
    <title><%= title%></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />

		<!-- CSS for Styling -->
		<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">

		<!-- JavaScript for Interactivity -->
		<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.min.js" integrity="sha384-wHAiFfRlMFy6i5SRaxvfOCifBUQy1xHdJ/yoi7FRNXMRBu5WHdZYu1hA6ZOblgut" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js" integrity="sha384-B0UglyR+jN6CkvvICOB2joaf5I4l3gm9GU6Hc1og6Ls7i6U/mkkaduKaBhlAXv9k" crossorigin="anonymous"></script>

  </head>
  <body>
    <h1> Pet Page </h1>
	<form id="html_form"  role="form" method="post" >
		<div>
			<table border="1px">
				<thead>
				<tr>
					<th scope="col"> Pet ID</th>
				</tr>
				</thead>
				<tbody>
				<% for(var i=0; i<pet.length ; i++) { %>
					<tr>
						<td><%= pet[i].petid%></td>
					</tr>
					<% } %>
				</tbody>
			</table>
		</div>

		<div>
			<table border="1px">
				<thead>
				<tr>
					<th scope="col"> Pet Name</th>
				</tr>
				</thead>
				<tbody>
				<% for(var i=0; i<pet.length ; i++) { %>
					<tr>
						<td><%= pet[i].name%></td>
					</tr>
					<% } %>
				</tbody>
			</table>
		</div>

		<div>
			<table border="1px">
				<thead>
				<tr>
					<th scope="col"> Pet Category</th>
				</tr>
				</thead>
				<tbody>
				<% for(var i=0; i<pet.length ; i++) { %>
					<tr>
						<td><%= pet[i].category%></td>
					</tr>
					<% } %>
				</tbody>
			</table>
		</div>

		<div>
			<table border="1px">
				<thead>
				<tr>
					<th scope="col"> Requirements</th>
				</tr>
				</thead>
				<tbody>
				<% for(var i=0; i<pet.length ; i++) { %>
					<tr>
						<td><%= pet[i].requirements%></td>
					</tr>
					<% } %>
				</tbody>
			</table>
		</div>
		<a href="../../../test"><button class="btn btn-primary">Edit Pet Profile</button></a> <!-- TODO: set to edit pet profile -->
		<a href="../../../test"><button class="btn btn-secondary">Back to pets</button></a> <!--TODO: set to pets page -->

	</form>



  </body>
</html>